options swap_delay=12 iteration=1000;

yanfei char lvl=90/90 cons=6 talent=9,9,9;
yanfei add weapon="thewidsith" refine=3 lvl=90/90;
yanfei add set="mh" count=4;
yanfei add stats hp=4780 atk=311 em=187 pyro%=0.466 cr=0.311; #main
yanfei add stats def%=0.124 def=39.36 hp=507.88 hp%=0.0992 atk=33.08 atk%=0.1488 er=0.1102 em=99.1 cr=0.2648 cd=0.7944;

bennett char lvl=90/90 cons=6 talent=9,9,9;
bennett add weapon="thealleyflash" refine=1 lvl=90/90;
bennett add set="sodp" count=4;
bennett add stats hp=4780 atk=311 hp%=0.466 hp%=0.466 heal=0.359;
bennett add stats def%=0.124 def=39.36 hp=507.88 hp%=0.0992 atk=33.08 atk%=0.0992 er=0.4408 em=138.74 cr=0.3641 cd=0.1324;

furina char lvl=90/90 cons=0 talent=9,9,9;
furina add weapon="festeringdesire" refine=5 lvl=90/90;
furina add set="gt" count=4;
furina add stats hp=4780 atk=311 hp%=0.466 hp%=0.466 cd=0.622;
furina add stats def%=0.124 def=39.36 hp=507.88 hp%=0.0992 atk=33.08 atk%=0.0992 er=0.551 em=39.64 cr=0.331 cd=0.3972;

yelan char lvl=90/90 cons=0 talent=9,9,9;
yelan add weapon="fav bow" refine=3 lvl=90/90;
yelan add set="eosf" count=4;
yelan add stats hp=4780 atk=311 hp%=0.466 hydro%=0.466 cr=0.311;
yelan add stats def%=0.124 def=39.36 hp=507.88 hp%=0.1488 atk=33.08 atk%=0.0992 er=0.2204 em=39.64 cr=0.331 cd=0.7282;

target lvl=100 resist=0.1 radius=2 pos=0,2.4 hp=999999999; 
energy every interval=480,720 amount=1;

active furina;
for let i=0; i<4; i=i+1 {
	furina skill, dash, burst;
	
	yelan skill, burst, attack;
	while !.yelan.mods.favonius-cd {
		yelan attack;
	}
	
	bennett burst, attack, skill;
	
	yanfei skill, attack;
	if .yanfei.burst.ready {
		yanfei burst;
	} else {
		yanfei attack;
	}
	yanfei attack, charge,
		attack:2, charge,
		attack:3, charge,
		attack:2, charge,
		attack, skill, charge;
		
	bennett skill;
}
